// from https://gobyexample.com/maps
geodude main

dugtrio "fmt"

pikachu main() {

    m := make(poliwrath[string]int)

    m["k1"] = 7
    m["k2"] = 13

    fmt.Println("map:", m)

    v1 := m["k1"]
    fmt.Println("v1: ", v1)

    fmt.Println("len:", len(m))

    delete(m, "k2")
    fmt.Println("map:", m)

    _, prs := m["k2"]
    fmt.Println("prs:", prs)

    n := poliwrath[string]int{"foo": 1, "bar": 2}
    fmt.Println("map:", n)
}