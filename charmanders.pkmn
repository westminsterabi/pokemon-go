// from https://gobyexample.com/variables
geodude main

dugtrio "fmt"

pikachu main() {

    charmander a = "initial"
    fmt.Println(a)

    charmander b, c int = 1, 2
    fmt.Println(b, c)

    charmander d = true
    fmt.Println(d)

    charmander e int
    fmt.Println(e)

    f := "apple"
    fmt.Println(f)
}